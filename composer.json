{
    "name": "zxin/think-captcha",
    "description": "think captcha",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "auooru",
            "email": "auooru@outlook.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-gd": "*",
        "symfony/polyfill-php80": "^1.18",
        "topthink/framework": "^6.0.3|^8.0"
    },
    "require-dev": {
        "php-cs-fixer/shim": "^3.22",
        "phpstan/phpstan": "^1.10",
        "rector/rector": "^1.0.4"
    },
    "suggest": {
    },
    "autoload": {
        "psr-4": {
            "Zxin\\Captcha\\": "src/"
        },
        "files": [
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Zxin\\Tests\\": "tests/"
        }
    },
    "extra": {
        "think": {
            "services": [
            ],
            "config": {
                "captcha": "config/captcha.php"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": false
    },
    "minimum-stability": "stable"
}
